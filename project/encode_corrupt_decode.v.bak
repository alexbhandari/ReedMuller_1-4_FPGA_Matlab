module encode_corrupt_decode (audio_in, aclk, clk, audio_out);
	input [5:0] audio_in;
	input aclk;
	input clk;
	output [5:0] audio_out;

	wire [5:0] ec;
	wire [5:0] cd;

	encode	e	(	.message(audio_in),		.aclk(aclk), .clk(clk), .codeword(ec)			);
	corrupt	c	(	.audio_in(ec),			.aclk(aclk), .clk(clk), .audio_out(de)			);
	decode	d	(	.audio_in(de),			.aclk(aclk), .clk(clk), .audio_out(audio_out)	);

endmodule